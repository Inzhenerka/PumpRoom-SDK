{
  "name": "pumproom-sdk",
  "version": "0.1.4",
  "description": "Lightweight SDK for integrating Learning Management Systems with PumpRoom platform",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/bundles/pumproom-sdk-latest.esm.js",
      "require": "./dist/bundles/pumproom-sdk-latest.umd.js"
    }
  },
  "files": [
    "dist/bundles",
    "dist/types",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "dev": "vite",
    "test": "vitest run --coverage",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@vitest/coverage-istanbul": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "type": "module",
  "sideEffects": true,
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "license": "UNLICENSED",
  "author": {
    "name": "Inzhenerka.Tech",
    "email": "info@inzhenerka.tech",
    "url": "https://inzhenerka.tech"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inzhenerka/PumpRoom-SDK.git"
  },
  "homepage": "https://sdk.pumproom.tech",
  "keywords": [
    "pumproom",
    "lms",
    "sdk",
    "integration",
    "education",
    "ai"
  ]
}
